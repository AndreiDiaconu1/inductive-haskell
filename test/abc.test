NEX target ((1:2:nil):(1:2:3:4:nil):nil) => ((5:nil):nil) ;;
NEX target ((1:2:5:nil):(2:nil):nil) => ((1:2:3:4:nil):nil) ;;

PEX target ((1:2:nil):(2:3:nil):(3:4:nil):nil) => ((1:nil):(2:nil):nil) ;;
PEX target ((5:6:nil):(6:nil):nil) => ((5:nil):nil) ;;

