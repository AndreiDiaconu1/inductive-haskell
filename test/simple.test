val comp(f, g) = lambda (x) f(g(x)) ;;

rec map(f) = lambda (xs)
    (if xs = nil
    then nil
    else f(head(xs)):map(f)(tail(xs))) ;;

val BK_addOne(x) = BK_addTwo(x) - 1 ;;

val BK_addTwo(x) = x + 2 ;;

