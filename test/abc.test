--PEX target (1:2:3:nil) => (6:5:4:nil) ;;
--PEX target (4:5:nil) => (8:7:nil) ;;

val addOne(x) = x + 1;;

val addTwo(x) = x + 2;;

val isOdd(x) = if x mod 2 = 0 
               then false
               else true;;

val id(x) = x;;

rec reverse1(xs, ys) = if xs = nil
                       then ys
                       else reverse1(tail(xs), head(xs):ys);;

val tl(xs) = if xs = nil then nil else tail(xs);;

val reverse(xs) = reverse1(xs, nil);;

SYNTH target;;